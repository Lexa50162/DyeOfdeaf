local MiscTab = Window:CreateTab("Misc", 4483362458) -- Title, Image


local MiscButton = MiscTab:CreateButton({
   Name = "fullbright",
   Callback = function()
   local Players = game:GetService("Players")
local Lighting = game:GetService("Lighting")
local UserInputService = game:GetService("UserInputService")
local player = Players.LocalPlayer

-- GUI
local gui = Instance.new("ScreenGui")
gui.Name = "FullbrightGui"
gui.ResetOnSpawn = false
gui.IgnoreGuiInset = true
gui.Parent = player:WaitForChild("PlayerGui")

-- Draggable Frame
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 160, 0, 30)
frame.Position = UDim2.new(0, 15, 0, 120)
frame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
frame.BorderSizePixel = 0
frame.Active = true
frame.Parent = gui

-- Fullbright Toggle Button
local button = Instance.new("TextButton")
button.Size = UDim2.new(0.75, 0, 1, 0)
button.Position = UDim2.new(0, 0, 0, 0)
button.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
button.TextColor3 = Color3.new(1, 1, 1)
button.Text = "FB: OFF"
button.Font = Enum.Font.Gotham
button.TextSize = 14
button.Parent = frame

-- Close "X" Button
local close = Instance.new("TextButton")
close.Size = UDim2.new(0.25, 0, 1, 0)
close.Position = UDim2.new(0.75, 0, 0, 0)
close.BackgroundColor3 = Color3.fromRGB(85, 20, 20)
close.TextColor3 = Color3.new(1, 1, 1)
close.Text = "X"
close.Font = Enum.Font.GothamBold
close.TextSize = 14
close.Parent = frame

-- Lighting logic
local fullbright = false
local original = {
	Ambient = Lighting.Ambient,
	OutdoorAmbient = Lighting.OutdoorAmbient,
	Brightness = Lighting.Brightness,
	ClockTime = Lighting.ClockTime,
	GlobalShadows = Lighting.GlobalShadows,
}

local function toggleFullbright(on)
	if on then
		Lighting.Ambient = Color3.new(1, 1, 1)
		Lighting.OutdoorAmbient = Color3.new(1, 1, 1)
		Lighting.Brightness = 3
		Lighting.ClockTime = 12
		Lighting.GlobalShadows = false
	else
		for k, v in pairs(original) do
			Lighting[k] = v
		end
	end
end

button.MouseButton1Click:Connect(function()
	fullbright = not fullbright
	toggleFullbright(fullbright)
	button.Text = fullbright and "FB: ON" or "FB: OFF"
end)

-- Close button removes GUI
close.MouseButton1Click:Connect(function()
	gui:Destroy()
end)

-- Drag support
local dragging, dragStart, startPos, dragInput
local function update(input)
	local delta = input.Position - dragStart
	frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
		startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

frame.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = frame.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

frame.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if dragging and input == dragInput then
		update(input)
	end
end)

   end,
})

local MiscButton = MiscTab:CreateButton({
   Name = "instant Promximity Prompt",
   Callback = function()
for _, prom in next, workspace:GetDescendants() do
  if prom:IsA("ProximityPrompt") then
    prom.PromptButtonHoldBegan:Connect(function()
       if prom.HoldDuration <= 0 then return end
      fireproximityprompt(prom, 0)
    end)
  end
end

workspace.DescendantAdded:Connect(function(class)
  if class:IsA("ProximityPrompt") then
    class.PromptButtonHoldBegan:Connect(function()
        if class.HoldDuration <= 0 then return end
      fireproximityprompt(class, 0)
    end)
  end
end)
   end,
})


local PlayerButton = PlayerTab:CreateButton({
   Name = "teleport gui (better in one bounce and survivor round",
   Callback = function()
  local Players = game:GetService("Players")
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")
local gui = Instance.new("ScreenGui")
gui.Name = "TeleportUI"
gui.ResetOnSpawn = false
gui.Parent = playerGui
local dragFrame = Instance.new("Frame")
dragFrame.Size = UDim2.new(0, 200, 0, 130)
dragFrame.Position = UDim2.new(0, 20, 0, 100)
dragFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
dragFrame.BackgroundTransparency = 0.2
dragFrame.Active = true
dragFrame.Draggable = true
dragFrame.Parent = gui
local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, 0, 0, 30)
title.BackgroundTransparency = 1
title.Text = "Teleport GUI"
title.TextScaled = true
title.TextColor3 = Color3.new(1, 1, 1)
title.Font = Enum.Font.GothamBold
title.Parent = dragFrame
local antiVoidPart = Instance.new("Part")
antiVoidPart.Name = "partthatcanmakeyoualwayswininonebounceorsurvivorround"
antiVoidPart.Size = Vector3.new(500000, 1, 500000)
antiVoidPart.Position = Vector3.new(0, 100000, 0) -- High in sky
antiVoidPart.Anchored = true
antiVoidPart.Transparency = 0.5
antiVoidPart.Parent = workspace
local originalCFrame = nil
local toVoidBtn = Instance.new("TextButton")
toVoidBtn.Size = UDim2.new(1, -20, 0, 40)
toVoidBtn.Position = UDim2.new(0, 10, 0, 40)
toVoidBtn.Text = "teleport to safezone like a good guy"
toVoidBtn.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
toVoidBtn.TextScaled = true
toVoidBtn.Font = Enum.Font.GothamBold
toVoidBtn.TextColor3 = Color3.new(1, 1, 1)
toVoidBtn.Parent = dragFrame

local backBtn = Instance.new("TextButton")
backBtn.Size = UDim2.new(1, -20, 0, 40)
backBtn.Position = UDim2.new(0, 10, 0, 85)
backBtn.Text = "go back to your deaf position"
backBtn.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
backBtn.TextScaled = true
backBtn.Font = Enum.Font.GothamBold
backBtn.TextColor3 = Color3.new(1, 1, 1)
backBtn.Parent = dragFrame


toVoidBtn.MouseButton1Click:Connect(function()
	local char = player.Character or player.CharacterAdded:Wait()
	local hrp = char:WaitForChild("HumanoidRootPart")
	originalCFrame = hrp.CFrame
	hrp.CFrame = antiVoidPart.CFrame + Vector3.new(0, 5, 0)
end)


backBtn.MouseButton1Click:Connect(function()
	if originalCFrame then
		local char = player.Character or player.CharacterAdded:Wait()
		local hrp = char:WaitForChild("HumanoidRootPart")
		hrp.CFrame = originalCFrame
	end
end)


local UIS = game:GetService("UserInputService")
local dragging, dragInput, dragStart, startPos

dragFrame.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = dragFrame.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

dragFrame.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

UIS.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		local delta = input.Position - dragStart
		dragFrame.Position = UDim2.new(
			startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y
		)
	end
end)

   end,
})

